<script type="text/javascript">
function validate_manual_login() {
	return_value = true;
	$(".error-message-custom").hide();
	
	if (!$('#UserUsername').val()) {
		buildMessage('UserUsername', 'Username is mandatory');
		return_value = false;
	}

	if (!$('#UserPassword').val()) {
		buildMessage('UserPassword', 'Password is mandatory');
		return_value = false;
	}
	
	return return_value;
}
</script>    
